<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<link rel="stylesheet" href="/assets/dashboard_orders.css">
	<title>Order Page</title>
</head>
<body>
	<!-- SIDEBAR -->
    <%- include ('dashboard_sidebar') -%>
    
	<section id="content">
		<nav>
			<i class='bx bx-menu toggle-sidebar' ></i>
			<form action="#">
				<div class="form-group">
				</div>
			</form>
			<a href="#" class="nav-link">
				<i class='bx bxs-bell icon' ></i></a>
			<div class="profile">
				<img src="/images/system_images/user.png" alt="">
				<ul class="profile-link">
					<li><a href="#"><i class='bx bxs-user-circle icon' ></i> Account</a></li>
					<li><a href="#"><i class='bx bxs-cog' ></i>Settings</a></li>
					<li><a href="#"><i class='bx bxs-log-out-circle' ></i> Logout</a></li>
			</div>
		</nav>
		<div class="dash-content">
            <div class="overview">
                <div class="title">
			<h1 class="title">Your Ongoing Rentals</h1>
			</div>
		<div class="boxes"  style="overflow-x:auto;">
            <table class="table table-bordered">
                <thead>
                  <tr>
                    <th scope="col"><input type="button" onclick='selects()' value="✓"/><input type="button" onclick='deSelect()' value="✕"/>   </th>
                    <th scope="col">ID</th>
                    <th scope="col">Purchase Point</th>
                    <th scope="col">Purchase Date</th>
                    <th scope="col">Bill-to Name</th>
                    <th scope="col">Ship-to Name</th>
                    <th scope="col">Grand Total</th>
                    <th scope="col">Status</th>
                    <th scope="col">Start Date</th>
                    <th scope="col">End Date</th>
                    <th scope="col">Action (buttons to update status)</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th scope="row"><input type="checkbox" name="chk" value="1"></th>
                    <td class="id">000012</td>
                    <td class="address">San Mateo, Rizal</td>
                    <td class="Pdate">October 3, 2022</td>
                    <td class="btName">Ivan Kyle</td>
                    <td class="stName">Ivan Kyle</td>
                    <td class="gtotal">₱ 1,099.00</td>
                    <td class="status">Pending</td>
                    <td class="startdate">November 4, 2022 12:00:00AM</td>
                    <td class="enddate">November 7, 2022 12:00:00PM</td>
                    <td class="action"><div class="p-1">
                      <button class="btn btn-primary button-1" data-id="1">On Going</button><br>
                      <button class="btn btn-primary button-2" data-id="2">Received by Customer</button><br>
                      <button class="btn btn-primary button-3" data-id="3">Received by Owner</button>
                      <p id="status" class="py-1"></p>
                    </div></td>
                  </tr>
                  <tr>
                    <th scope="row"><input type="checkbox" name="chk" value="2"></th>
                    <td class="id">000013</td>
                    <td class="address">Marikina City</td>
                    <td class="Pdate">October 10, 2022</td>
                    <td class="btName">Richelieu T</td>
                    <td class="stName">Richelieu T</td>
                    <td class="gtotal">₱ 879.00</td>
                    <td class="status">Pending</td>
                    <td class="startdate">November 8, 2022 12:00:00AM</td>
                    <td class="enddate">November 10, 2022 12:00:00PM</td>
                    <td class="action"><div class="p-1">
                      <button class="btn btn-primary button-1" data-id="1">On Going</button><br>
                      <button class="btn btn-primary button-2" data-id="2">Received by Customer</button><br>
                      <button class="btn btn-primary button-3" data-id="3">Received by Owner</button>
                      <p id="status" class="py-1"></p>
                    </div></td>
                  </tr>
                  <tr>
                    <th scope="row"><input type="checkbox" name="chk" value="3"></th>
                    <td class="id">000014</td>
                    <td class="address">Manila, City</td>
                    <td class="Pdate">October 20, 2022</td>
                    <td class="btName">Yanna Mari</td>
                    <td class="stName">Robert Delim</td>
                    <td class="gtotal">₱ 2,979.00</td>
                    <td class="status">Pending</td>
                    <td class="startdate">November 20, 2022 12:00:00AM</td>
                    <td class="enddate">November 28, 2022 12:00:00PM</td>
                    <td class="action"><div class="p-1">
                      <button class="btn btn-primary button-1" data-id="1">On Going</button><br>
                      <button class="btn btn-primary button-2" data-id="2">Received by Customer</button><br>
                      <button class="btn btn-primary button-3" data-id="3">Received by Owner</button>
                      <p id="status" class="py-1"></p>
                    </div></td>
                  </tr>
                </tbody>
              </table>
		</div>
	</section>
	<script src="/assets/scripts/dashboard_orders.js"></script>
</body>
</html>